            $('.status-description').append(\"Todos los sistemas funcionales\");\n                    $('#status_flag_description').append(\"Todos los sistemas funcionales\");\n                } else if(description === 'Major System Outage'){\n                    $('.status-description').append(\"Falla generalizada del sistema\");\n                    $('#status_flag_description').append(\"Falla generalizada del sistema\");\n                } else if(description === 'Partial System Outage'){\n                    $('.status-description').append(\"Falla parcial del sistema\");\n                    $('#status_flag_description').append(\"Falla parcial del sistema\");\n                } else if(description === 'Minor Service Outage'){\n                    $('.status-description').append(\"Falla menor del sistema\");\n                    $('#status_flag_description').append(\"Falla menor del sistema\");\n                } else if(description === 'Degraded System Service'){\n                    $('.status-description').append(\"Intermitencia en servicio del sistema\");\n                    $('#status_flag_description').append(\"Intermitencia en servicio del sistema\");\n                } else if(description === 'Partially Degraded Service'){\n                    $('.status-description').append(\"Intermitencia parcial en servicio del sistema\");\n                    $('#status_flag_description').append(\"Intermitencia parcial en servicio del sistema\");\n                } else if(description === 'Service Under Maintenance'){\n                    $('.status-description').append(\"Servicio bajo mantenimiento\");\n                    $('#status_flag_description').append(\"Servicio bajo mantenimiento\");\n                }\n            }\n            function processSign(indicator){\n                if(indicator === \"critical\"){\n                    $('#flag_status_light, #footer_status_light').css(\"background-color\",\"#e74c3c\");\n                } else if(indicator === \"