umentation\n        // for FB.getLoginStatus().\n        if (response.status === 'connected') {\n            // Logged into your app and Facebook.\n            fb_status = 'connected';\n            fb_accessToken = response.authResponse.accessToken;\n            testAPI();\n        } else if (response.status === 'not_authorized') {\n            fb_status = 'not_authorized';\n            // The person is logged into Facebook, but not your app.\n        } else {\n            fb_status = 'unknown';\n            // The person is not logged into Facebook, so we're not sure if\n            // they are logged into this app or not.\n        }\n//console.log(\"statusCheckCallback: fb_status \" + fb_status);\n    }\n\n    // This function is called when someone finishes with the Login\n    // Button.  See the onlogin handler attached to it in the sample\n    // code below.\n    function checkLoginState() {\n        FB.getLoginStatus(function(response) {\n            statusCheckCallback(response);\n        });\n    }\n\n    window.fbAsyncInit = function() {\n        FB.init({\n            appId      : 394586223946879,\n            cookie     : true,  // enable cookies to allow the server to access\n                                // the session\n            xfbml      : true,  // parse social plugins on this page\n            version    : 'v2.6', // use version 2.2\n            status     : false\n\n            //,status     : true\n            // Rather than calling FB.getLoginStatus explicitly, it is possible to check the user's status by setting \"status: true\" when you call FB.init.\n            // To receive the response of this call, you must subscribe to the auth.statusChange event.\n            // The response object passed by this event is identical to that which would be returned by calling FB.getLoginStatus explicitly.\n            // when \"status: true\", the response object will be cached by the SDK.\n            // BUT... just use FB.getLoginStatus explicitly instead???\n\n        });\n\n        FB.getLoginStatus(function(response) {\n            statusCheckCallback(response);\n        });\n\n\n\n        FB.Event.subscribe('messenger_checkbox', function(e) {\n            console.log(\"messenger_checkbox event\");\n            console.log(e);\n\n            if (e.event == 'rendered') {\n                console.log(\"Plugin was rendered\");\n            } else if (e.event == 'checkbox') {\n                var checkboxState = e.state;\n                console.log(\"Checkbox state: \" + checkboxState);\n            } else if (e.event == 'not_you') {\n                console.log(\"User clicked 'not you'\");\n            } else if (e.event == 'hidden') {\n                console.log(\"Plugin was hidden\");\n            }\n\n        });\n\n    };\n\n    // Load the SDK asynchronously\n    (function(d, s, id) {\n        var js, fjs = d.getElementsByTagName(s)[0];\n        if (d.getElementById(id)) return;\n        js = d.createElement(s); js.id = id;\n        js.src = \"//connect.facebook.net/en_US/sdk.js\";\n        fjs.parentNode.insertBefore(js, fjs);\n    }(document, 'script', 'facebook-jssdk'));\n\n\n    // Here we run a very simple test of the Graph API after login is\n    // successful.  See statusCheckCallback() for when this call is made.\n    function testAPI() {\n//console.log('testAPI called');\n        FB.api('/me?fields=name,email', function(response) {\n//console.log('Successful check for: ' + response.name);\n//console.log('testAPI response:');\n//console.log(response);\n            fb_id       = response.id;\n            fb_name     = response.name;\n            fb_email    = response.email;\n\n//console.log('testAPI show_status:');\n            show_status();\n        });\n    }\n\n\n    var fblogin = function() {\n        checkLoginState();    // init fb_ vars\n\n//console.log(\"fblogin show_status: \");\n        show_status();\n\n        if( fb_status != 'connected' ) {\n//console.log(\"fblogin: not connected\");\n            FB.login(function (response) {\n                if (response.status === 'connected') {\n                // Logged into your app and Facebook.\n//console.log(\"fblogin response: \" );\n//console.log(response);\n                    fb_status = 'connected';\n                    fb_accessToken = response.authResponse.accessToken;\n                    fb_id       = response.id;\n                    fb_name     = response.name;\n                    fb_email    = response.email;\n//console.log(\"fblogin call FaceBookRegister\");\n                    FaceBookRegister();\n\n                } else if (response.status === 'not_authorized') {\n                        // The person is logged into Facebook, but not your app.\n                } else {\n                        // The person is not logged into Facebook, so we're not sure if\n                        // they are logged into this app or not.\n                }\n            } , {scope: 'public_profile,email'});\n        } else {\n//console.log(\"fblogin call FaceBookRegister 2\");\n            FaceBookRegister();\n        }\n/*\nconsole.log(\"fblogin show_status 2: \");\n        show_status();\n\n        if(fb_status === 'connected'){\n            if( dsid === 0 ) {\n            // user is NOT ds logged in\n                FaceBookRegister();\n            }\n        }\n*/\n\n    };\n\n\n    var fb_re_login = function() {\n//console.log('fb_re_login');\n/*\n        FB.login(function(response) {\n            checkUser();\n        }, { scope: 'public_profile,email' , auth_type: 'rerequest' })\n*/\n    };\n\n\n    var fblogout = function() {\n        if (fb_status === 'connected') {\n            //FB.logout(function(response){\n            //});\n            fb_status = 'logged out';\n            sessionStorage.removeItem(\"UserName\");\n            sessionStorage.removeItem(\"successredir\");\n            window.location.href = \"/account/logout.php\";\n\n        } else {\n            fb_status = 'logged out';\n            sessionStorage.removeItem(\"UserName\");\n            sessionStorage.removeItem(\"successredir\");\n            window.location.href = \"/account/logout.php\";\n        }\n    };\n\n\n    function show_status(){\n    /*\n        console.log(\"show_status fb_status: \" + fb_status);\n        console.log('show_status fb_id: ' + fb_id);\n        console.log('show_status fb_name: ' + fb_name);\n        console.log('show_status fb_email: ' + fb_email);\n        console.log('show_status dsid: ' + dsid);\n    */\n    }\n\n\n    ////////////////////////////////        AUTHENTICATE THE FACEBOOK USER ON THE SERVER    ///////////////////////////////\n\n    function FaceBookRegister() {\n        FB.api('/me?fields=id,name,first_name,last_name,email', function(response) {\n//console.log('FaceBookRegister for: ' + response.name);\n//console.log(response);\n            fb_id       = response.id;\n            fb_name     = response.name;\n            fb_email    = response.email;\n\n//console.log('FaceBookRegister ' + response);\n//console.log('FaceBookRegister name: ' + response.first_name);\n//console.log('FaceBookRegister name: ' + response.last_name);\n//console.log('FaceBookRegister id: ' + response.id);\n//console.log('FaceBookRegister email: ' + response.email);\n//console.log('FaceBookRegister href: ' + window.location.href);\n\n//console.log('FaceBookRegister sess dsid: ' + dsid);\n\n\n//console.log('FaceBookRegister show_status:');\n            show_status();\n//alert('FaceBookRegister: ' + fb_status );\n            if ( (fb_status === 'connected') && (fb_id != 'undefined') && (fb_email != 'undefined') )  {\n            //if(response.status === 'connected') {\n//console.log('FaceBookRegister logged in');\n                var logpopredirjs = sessionStorage.getItem(\"successredir\");\n//alert(\"logpopredirjs: \" + logpopredirjs);\n//console.log('FaceBookRegister logpopredirjs: ' + logpopredirjs );\n                if( dsid === 0) {\n//console.log('FaceBookRegister dsid === 0');\n                    form = document.createElement(\"form\");\n                    form.setAttribute(\"method\", 'post');\n                    form.setAttribute(\"action\", '/account/login.php');\n                    //form.setAttribute(\"action\", window.location.href);\n\n                    if(logpopredirjs !== null) {\n                        var field = document.createElement(\"input\");\n                        field.setAttribute(\"type\", \"text\");\n                        field.setAttribute(\"name\", 'fb_logpopredir');\n                        field.setAttribute(\"value\", logpopredirjs);\n//console.log('FaceBookRegister field logpopredirjs: ' + logpopredirjs );\n                        form.appendChild(field);\n                    }\n\n                    var field = document.createElement(\"input\");\n                    field.setAttribute(\"type\", \"text\");\n                    field.setAttribute(\"name\", 'fb_accessToken');\n                    field.setAttribute(\"value\", fb_accessToken);\n//console.log('FaceBookRegister field fb_accessToken: ' + fb_accessToken );\n                    form.appendChild(field);\n\n                    var field = document.createElement(\"input\");\n                    field.setAttribute(\"type\", \"text\");\n                    field.setAttribute(\"name\", 'fb_id');\n                    field.setAttribute(\"value\", response.id);\n//console.log('FaceBookRegister field response.id: ' + response.id );\n                    form.appendChild(field);\n\n                    var field = document.createElement(\"input\");\n                    field.setAttribute(\"type\", \"text\");\n                    field.setAttribute(\"name\", 'fb_email');\n                    field.setAttribute(\"value\", response.email);\n//console.log('FaceBookRegister field response.email: ' + response.email );\n                    form.appendChild(field);\n\n                    var field = document.createElement(\"input\");\n                    field.setAttribute(\"type\", \"text\");\n                    field.setAttribute(\"name\", 'fb_first_name');\n                    field.setAttribute(\"value\", response.first_name);\n//console.log('FaceBookRegister field response.first_name: ' + response.first_name );\n                    form.appendChild(field);\n\n                    var field = document.createElement(\"input\");\n                    field.setAttribute(\"type\", \"text\");\n                    field.setAttribute(\"name\", 'fb_last_name');\n                    field.setAttribute(\"value\", response.last_name);\n//console.log('FaceBookRegister field response.last_name: ' + response.last_name );\n                    form.appendChild(field);\n\n                    document.body.appendChild(form);\n                    sessionStorage.removeItem(\"successredir\");\n                    form.submit();\n                }\n            } else {\n//console.log('FaceBookRegister NOT logged in');\n//alert('You must first log in with your Facebook account!');\n                sessionStorage.removeItem(\"successredir\");\n                var logpopredirjs = sessionStorage.getItem(\"failredir\");\n                FacebookLogoutLocal();\n            }\n        });\n    }\n\n    ////////////////////////////////           LOGS THE USER OUT FROM THE SERVER              ////////////////////////////\n\n    function FacebookLogoutLocal() {\n        sessionStorage.removeItem(\"UserName\");\n        sessionStorage.removeItem(\"successredir\");\n//        if (loggedIn == false) {\n            form = document.createElement(\"form\");\n            form.setAttribute(\"method\", 'post');\n            form.setAttribute(\"action\", '/account/logout.php');\n            document.body.appendChild(form);\n            form.submit();\n//        }\n    }\n\n\n    </script><script>\n    dataLayer.push({\n        'event': 'eventHandlerFire',\n        'eventCategory': 'AB Tests',\n        'eventAction': 'Test 32905',\n        'eventLabel': 'Variant 1 (32905)',\n        'nonInteraction': true\n    });\n</script>\n    <script type=\"text/javascript\">\n                //create json object for each popup used in header.footer.js\n        var popUpBrands = [];\n        var popUpTypes = [];\n        var popUpConditions = [];\n        var popUpIngredients = [];\n    </script>\n\n\n<script src=\"//media.dermstore.com/images/scripts/js.cookie.min.js\"></script>\n<script data-note='new JS cookie get/set'>\n    function ePopToggle(){\n        var show = arguments.length < 1 ? true : arguments[0];\n        if( show ){\n            ePopSetup();\n            $('#stickyFooter').hide();\n            $('#emailPop_wrapper').show();\n            $('#emailPop-new').modal('show');\n            $.ajax({\n                url: '/ajax/modal_activate_log.php',\n                type: 'POST',\n                success: function (data) {\n                    dataLayer.push({\n                        'event': 'eventHandlerFire',\n                        'eventCategory': 'Email Signup',\n                        'eventAction': 'Modal Form Generated'\n                    });\n                }\n            });\n        }\n        else $('#emailPop-new').modal('hide');\n        }\n    function ePopForce() {\n        if( !Cookies.getJSON('epop_seen') ) {\n            Cookies.set('epop_seen',true,{expires:7})\n            ePopToggle(true)\n            }\n        else ePopToggle(false);\n        }\n    function ePopSetup() {\n        \n        $('form#emailSubmit').submit(function (e) {\n            //get form data email_address\n            var formData = $(this).serialize();\n            //add loading gif\n            $('#initialPopContent').hide();\n            $('#displayError').hide();\n            $('#displayLoading').show();\n            //send ajax call\n            $.ajax({\n                url: '/ajax/emailpop_join_scroll.php',\n                data: formData,\n                type: 'POST',\n                succe