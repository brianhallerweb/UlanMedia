03b5\u03c2-\u03b7 \u03b1\u03c0\u03ce\u03bb\u03b5\u03b9\u03b1 \u03b2\u03ac\u03c1\u03bf\u03c5\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03ba\u03b1\u03b8\u03b1\u03c1\u03ac \u03b8\u03ad\u03bc\u03b1 \u03c3\u03c5\u03bd\u03ae\u03b8\u03b5\u03b9\u03b1\u03c2 \u03ba\u03b1\u03b9 \u03c6\u03c5\u03c3\u03b9\u03ba\u03ac \u03bf \u03c7\u03c1\u03cc\u03bd\u03bf\u03c2 \u03c0\u03bf\u03c5 \u03b8\u03b1 \u03b1\u03c0\u03b1\u03bb\u03bb\u03b1\u03b3\u03bf\u03cd\u03bd \u03b1\u03c0\u03cc \u03c4\u03b1 \u03c0\u03b5\u03c1\u03b9\u03c4\u03c4\u03ac \u03ba\u03b9\u03bb\u03ac \u03b5\u03be\u03b1\u03c1\u03c4\u03ac\u03c4\u03b1\u03b9 \u03ac\u03bc\u03b5\u03c3\u03b1 \u03b1\u03c0\u03cc \u03c4\u03b7\u03bd \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03b9\u03ba\u03ae \u03c4\u03bf\u03c5\u03c2 \u03c1\u03bf\u03c5\u03c4\u03af\u03bd\u03b1. \u039f \u03c7\u03c1\u03c5\u03c3\u03cc\u03c2 \u03ba\u03b1\u03bd\u03cc\u03bd\u03b1\u03c2 \u03cc\u03c4\u03b1\u03bd \u03b1\u03c0\u03bf\u03c6\u03b1\u03c3\u03af\u03b6\u03b5\u03c4\u03b5 \u03bd\u03b1 \u03c5\u03b9\u03bf\u03b8\u03b5\u03c4\u03ae\u03c3\u03b5\u03c4\u03b5 \u03c0\u03b9\u03bf \u03c5\u03b3\u03b9\u03b5\u03b9\u03bd\u03ad\u03c2 \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03b9\u03ba\u03ad\u03c2 \u03c3\u03c5\u03bd\u03ae\u03b8\u03b5\u03b9\u03b5\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03bd\u03b1 \u03bc\u03b7\u03bd \u03c4\u03b9\u03c2 \u03b1\u03bd\u03c4\u03b9\u03bc\u03b5\u03c4\u03c9\u03c0\u03af\u03c3\u03b5\u03c4\u03b5 \u03c9\u03c2 \u03ba\u03bf\u03bc\u03bc\u03ac\u03c4\u03b9 \u03bc\u03b9\u03b1\u03c2 &#171;\u03b4\u03af\u03b1\u03b9\u03c4\u03b1\u03c2&#187; \u03b1\u03bb\u03bb\u03ac \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae\u03c2, \u03b4\u03b7\u03bb\u03b1\u03b4\u03ae \u03b1\u03c0\u03bb\u03ae\u03c2, \u03ba\u03b1\u03b8\u03b7\u03bc\u03b5\u03c1\u03b9\u03bd\u03ae\u03c2 \u03b1\u03bd\u03ac\u03b3\u03ba\u03b7\u03c2. \u0397 \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae \u03b4\u03b5\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03b1\u03b3\u03b3\u03b1\u03c1\u03b5\u03af\u03b1, \u03b1\u03bb\u03bb\u03ac \u03c3\u03c5\u03bd\u03ae\u03b8\u03b5\u03b9\u03b1.<br />\n<br />\n*\u0397 \u03c3\u03c9\u03c3\u03c4\u03ae \u03bc\u03b5\u03c1\u03b9\u03b4\u03bf\u03c0\u03bf\u03af\u03b7\u03c3\u03b7 \u03c3\u03c4\u03b1 \u03b3\u03b5\u03cd\u03bc\u03b1\u03c4\u03b1 \u03c7\u03c9\u03c1\u03af\u03c2 \u03c5\u03c0\u03b5\u03c1\u03b2\u03bf\u03bb\u03ad\u03c2 \u03c3\u03c4\u03b9\u03c2 \u03c0\u03bf\u03c3\u03cc\u03c4\u03b7\u03c4\u03b5\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b5\u03c0\u03af\u03c3\u03b7\u03c2 \u03ad\u03bd\u03b1\u03c2 \u03c0\u03bf\u03bb\u03cd \u03c3\u03b7\u03bc\u03b1\u03bd\u03c4\u03b9\u03ba\u03cc\u03c2 \u03c0\u03b1\u03c1\u03ac\u03b3\u03bf\u03bd\u03c4\u03b1\u03c2 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b7 \u03c0\u03c1\u03bf\u03c3\u03c0\u03ac\u03b8\u03b5\u03b9\u03b1 \u03b1\u03c0\u03bf\u03c4\u03b5\u03bb\u03b5\u03c3\u03bc\u03b1\u03c4\u03b9\u03ba\u03ae.<br />\n*\u03a9\u03c3\u03c4\u03cc\u03c3\u03bf, \u03c4\u03bf \u03b2\u03b1\u03c3\u03b9\u03ba\u03cc\u03c4\u03b5\u03c1\u03bf \u03ba\u03bf\u03bc\u03bc\u03ac\u03c4\u03b9 \u03b5\u03bd\u03cc\u03c2 \u03c5\u03b3\u03b9\u03b5\u03b9\u03bd\u03bf\u03cd \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03bf\u03c2 \u03b1\u03c0\u03ce\u03bb\u03b5\u03b9\u03b1\u03c2 \u03b2\u03ac\u03c1\u03bf\u03c5\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b7 \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03c4\u03c1\u03bf\u03c6\u03ce\u03bd \u03bc\u03b5 \u03c3\u03c7\u03b5\u03c4\u03b9\u03ba\u03ac \u03c7\u03b1\u03bc\u03b7\u03bb\u03cc \u03b3\u03bb\u03c5\u03ba\u03b1\u03b9\u03bc\u03b9\u03ba\u03cc \u03b4\u03b5\u03af\u03ba\u03c4\u03b7, \u03c0\u03bf\u03c5 \u03ba\u03b1\u03c4\u03b1\u03bd\u03b1\u03bb\u03ce\u03bd\u03bf\u03bd\u03c4\u03b1\u03b9 \u03b5\u03c5\u03c7\u03ac\u03c1\u03b9\u03c3\u03c4\u03b1 \u03ba\u03b1\u03b9 \u03b5\u03b4\u03ce \u03b8\u03b1 \u03c7\u03c1\u03b5\u03b9\u03b1\u03c3\u03c4\u03b5\u03af \u03ad\u03c1\u03b5\u03c5\u03bd\u03b1 \u03ba\u03b1\u03b9 \u03c0\u03b9\u03b8\u03b1\u03bd\u03cc\u03c4\u03b1\u03c4\u03b1 \u03b7 \u03c3\u03c5\u03bc\u03b2\u03bf\u03bb\u03ae \u03b4\u03b9\u03b1\u03b9\u03c4\u03bf\u03bb\u03cc\u03b3\u03bf\u03c5 &#8211; \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03bf\u03bb\u03cc\u03b3\u03bf\u03c5.<br />\n*\u0388\u03bd\u03b1 \u03b1\u03c0\u03cc \u03c4\u03b1 \u03bc\u03b5\u03b3\u03b1\u03bb\u03cd\u03c4\u03b5\u03c1\u03b1 \u03b1\u03c4\u03bf\u03c0\u03ae\u03bc\u03b1\u03c4\u03b1 \u03c4\u03c9\u03bd \u03b1\u03bd\u03b4\u03c1\u03ce\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03b7 \u03c5\u03c0\u03b5\u03c1\u03c4\u03af\u03bc\u03b7\u03c3\u03b7 \u03c4\u03c9\u03bd \u03b1\u03bd\u03b1\u03b3\u03ba\u03ce\u03bd \u03c4\u03bf\u03c5 \u03bf\u03c1\u03b3\u03b1\u03bd\u03b9\u03c3\u03bc\u03bf\u03cd \u03c4\u03bf\u03c5\u03c2 \u03c3\u03b5 \u03c0\u03c1\u03c9\u03c4\u03b5\u0390\u03bd\u03b7. \u0394\u03b5\u03bd \u03b1\u03bd\u03b1\u03c6\u03b5\u03c1\u03cc\u03bc\u03b1\u03c3\u03c4\u03b5 \u03c6\u03c5\u03c3\u03b9\u03ba\u03ac \u03c3\u03b5 \u03cc\u03c3\u03bf\u03c5\u03c2 &#171;\u03c7\u03c4\u03af\u03b6\u03bf\u03c5\u03bd&#187; \u03bc\u03c5\u03b9\u03ba\u03ae \u03bc\u03ac\u03b6\u03b1 \u03ae \u03c4\u03bf\u03c5\u03c2 \u03b1\u03b8\u03bb\u03b7\u03c4\u03ad\u03c2 \u03b1\u03bb\u03bb\u03ac \u03c3\u03b5 \u03ac\u03bd\u03b4\u03c1\u03b5\u03c2 \u03bc\u03b5 \u03ba\u03b1\u03bd\u03bf\u03bd\u03b9\u03ba\u03ae \u03c6\u03c5\u03c3\u03b9\u03ba\u03ae \u03ba\u03b1\u03c4\u03ac\u03c3\u03c4\u03b1\u03c3\u03b7 \u03c0\u03bf\u03c5 \u03c3\u03c5\u03bd\u03ae\u03b8\u03c9\u03c2 \u03b4\u03b5\u03bd \u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03b5\u03c4\u03b1\u03b9 \u03bd\u03b1 \u03ba\u03b1\u03c4\u03b1\u03bd\u03b1\u03bb\u03ce\u03bd\u03bf\u03c5\u03bd \u03c0\u03b5\u03c1\u03b9\u03c3\u03c3\u03cc\u03c4\u03b5\u03c1\u03b5\u03c2 \u03c0\u03c1\u03c9\u03c4\u03b5\u03ca\u03bd\u03bf\u03cd\u03c7\u03b5\u03c2 \u03c4\u03c1\u03bf\u03c6\u03ad\u03c2 \u03c3\u03c5\u03b3\u03ba\u03c1\u03b9\u03c4\u03b9\u03ba\u03ac \u03bc\u03b5 \u03c4\u03b9\u03c2 \u03b3\u03c5\u03bd\u03b1\u03af\u03ba\u03b5\u03c2.<br />\n*\u039f \u03ba\u03b1\u03bd\u03cc\u03bd\u03b1\u03c2 \u03c0\u03bf\u03c5 \u03c0\u03c1\u03bf\u03c9\u03b8\u03b5\u03af \u03c4\u03b7\u03bd \u03ad\u03bd\u03c4\u03b1\u03be\u03b7 \u03bb\u03b1\u03c7\u03b1\u03bd\u03b9\u03ba\u03ce\u03bd \u03c0\u03b1\u03bd\u03c4\u03cc\u03c2 \u03c4\u03cd\u03c0\u03bf\u03c5 \u03c3\u03c4\u03bf \u03ba\u03b1\u03b8\u03b7\u03bc\u03b5\u03c1\u03b9\u03bd\u03cc \u03b4\u03b9\u03b1\u03b9\u03c4\u03bf\u03bb\u03cc\u03b3\u03b9\u03bf \u03b9\u03c3\u03c7\u03cd\u03b5\u03b9 \u03ba\u03b1\u03b9 \u03b3\u03b9\u03b1 \u03c4\u03b1 \u03b4\u03cd\u03bf \u03c6\u03cd\u03bb\u03bb\u03b1.<br />\n*\u0398\u03b5\u03c9\u03c1\u03bf\u03cd\u03bc\u03b5 \u03b1\u03c5\u03c4\u03bf\u03bd\u03cc\u03b7\u03c4\u03bf \u03cc\u03c4\u03b9 \u03b7 \u03b4\u03b9\u03b1\u03b4\u03b9\u03ba\u03b1\u03c3\u03af\u03b1 \u03b1\u03c0\u03ce\u03bb\u03b5\u03b9\u03b1\u03c2 \u03b2\u03ac\u03c1\u03bf\u03c5\u03c2 \u03b5\u03c0\u03b9\u03c4\u03b1\u03c7\u03cd\u03bd\u03b5\u03c4\u03b1\u03b9 \u03c3\u03c5\u03bd\u03b4\u03c5\u03b1\u03c3\u03c4\u03b9\u03ba\u03ac \u03bc\u03b5 \u03c6\u03c5\u03c3\u03b9\u03ba\u03ae \u03ac\u03c3\u03ba\u03b7\u03c3\u03b7 \u03ae \u03ad\u03c3\u03c4\u03c9 \u03ad\u03bd\u03b1 30\u03bb\u03b5\u03c0\u03c4\u03bf \u03c0\u03b5\u03c1\u03c0\u03ac\u03c4\u03b7\u03bc\u03b1 \u03c3\u03b5 \u03ba\u03b1\u03b8\u03b7\u03bc\u03b5\u03c1\u03b9\u03bd\u03ae \u03b2\u03ac\u03c3\u03b7.<br />\n<br />\n\u03a0\u03b1\u03c1\u03b1\u03ba\u03ac\u03c4\u03c9 \u03bb\u03bf\u03b9\u03c0\u03cc\u03bd \u03c0\u03b1\u03c1\u03b1\u03b8\u03ad\u03c4\u03bf\u03c5\u03bc\u03b5 \u03bc\u03b9\u03b1 \u03bb\u03af\u03c3\u03c4\u03b1 \u03bc\u03b5 \u03c0\u03c1\u03bf\u03c4\u03ac\u03c3\u03b5\u03b9\u03c2 \u03c0\u03bf\u03c5 \u03bc\u03c0\u03bf\u03c1\u03b5\u03af\u03c4\u03b5 \u03bd\u03b1 \u03b2\u03ac\u03bb\u03b5\u03c4\u03b5 \u03c3\u03c4\u03bf \u03c4\u03c1\u03b1\u03c0\u03ad\u03b6\u03b9 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03ba\u03ac\u03bd\u03b5\u03c4\u03b5 \u03bc\u03b9\u03b1 \u03c3\u03c9\u03c3\u03c4\u03ae \u03b1\u03c1\u03c7\u03ae. \u039a\u03b1\u03bb\u03ae \u03b5\u03c0\u03b9\u03c4\u03c5\u03c7\u03af\u03b1!<br />\n<script async=\"\" src=\"//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js\"></script>\n\n<ins class=\"adsbygoogle\" data-ad-client=\"ca-pub-3023702475348982\" data-ad-format=\"fluid\" data-ad-layout-key=\"-e9+4z+b1-h3+6d\" data-ad-slot=\"3213945841\" style=\"display: block;\"></ins><script>\n     (adsbygoogle = window.adsbygoogle || []).push({});\n</script>\n\n<br />\n<b>\u03a8\u03ac\u03c1\u03b9\u03b1</b><br />\n\u03a8\u03ac\u03c1\u03b9\u03b1 \u03cc\u03c0\u03c9\u03c2 \u03bf \u03c3\u03bf\u03bb\u03bf\u03bc\u03cc\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b9\u03b4\u03b1\u03bd\u03b9\u03ba\u03ae \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03b9\u03c3\u03bf\u03c1\u03c1\u03bf\u03c0\u03b7\u03bc\u03ad\u03bd\u03b7 \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae. \u03a0\u03b5\u03c1\u03b9\u03ad\u03c7\u03b5\u03b9 \u03a93 \u03bb\u03b9\u03c0\u03b1\u03c1\u03ac \u03bf\u03be\u03ad\u03b1 \u03c4\u03b1 \u03bf\u03c0\u03bf\u03af\u03b1 \u03b2\u03bf\u03b7\u03b8\u03bf\u03cd\u03bd \u03c3\u03c4\u03b7\u03bd \u03ba\u03b1\u03c4\u03b1\u03c0\u03bf\u03bb\u03ad\u03bc\u03b7\u03c3\u03b7 \u03c6\u03bb\u03b5\u03b3\u03bc\u03bf\u03bd\u03ce\u03bd \u03ba\u03b1\u03b9 \u03c3\u03c5\u03bc\u03b2\u03ac\u03bb\u03bb\u03bf\u03c5\u03bd \u03c3\u03c4\u03b7\u03bd \u03b1\u03c0\u03ce\u03bb\u03b5\u03b9\u03b1 \u03b2\u03ac\u03c1\u03bf\u03c5\u03c2 \u03ba\u03b1\u03b9 \u03c4\u03b7 \u03b4\u03b9\u03b1\u03c4\u03ae\u03c1\u03b7\u03c3\u03b7 \u03c4\u03b7\u03c2 \u03ba\u03b1\u03bb\u03ae\u03c2 \u03bb\u03b5\u03b9\u03c4\u03bf\u03c5\u03c1\u03b3\u03af\u03b1\u03c2 \u03c4\u03bf\u03c5 \u03bf\u03c1\u03b3\u03b1\u03bd\u03b9\u03c3\u03bc\u03bf\u03cd. \u0395\u03af\u03bd\u03b1\u03b9 \u03ac\u03c1\u03c1\u03b7\u03ba\u03c4\u03b1 \u03c3\u03c5\u03bd\u03b4\u03b5\u03b4\u03b5\u03bc\u03ad\u03bd\u03b1 \u03bc\u03b5 \u03c4\u03b7\u03bd \u03c0\u03c1\u03cc\u03bb\u03b7\u03c8\u03b7 \u03ba\u03b1\u03c1\u03b4\u03b9\u03b1\u03ba\u03ce\u03bd \u03bd\u03bf\u03c3\u03b7\u03bc\u03ac\u03c4\u03c9\u03bd \u03ba\u03b1\u03b8\u03ce\u03c2 \u03ba\u03b1\u03b9 \u03c4\u03c9\u03bd \u03c6\u03bb\u03b5\u03b3\u03bc\u03bf\u03bd\u03ce\u03bd \u03c4\u03c9\u03bd \u03b1\u03c1\u03b8\u03c1\u03ce\u03c3\u03b5\u03c9\u03bd. \u03a4\u03b5\u03bb\u03b5\u03c5\u03c4\u03b1\u03af\u03b1 \u03b2\u03ad\u03b2\u03b1\u03b9\u03b1 \u03bf \u03c3\u03bf\u03bb\u03bf\u03bc\u03cc\u03c2 \u03ad\u03c7\u03b5\u03b9 \u03b5\u03bd\u03bf\u03c7\u03bf\u03c0\u03bf\u03b9\u03b7\u03b8\u03b5\u03af \u03b3\u03b9\u03b1 \u03c5\u03c8\u03b7\u03bb\u03ad\u03c2 \u03c3\u03c5\u03b3\u03ba\u03b5\u03bd\u03c4\u03c1\u03ce\u03c3\u03b5\u03b9\u03c2 \u03b2\u03b1\u03c1\u03ad\u03c9\u03bd \u03c4\u03bf\u03be\u03b9\u03ba\u03ce\u03bd \u03bc\u03b5\u03c4\u03ac\u03bb\u03bb\u03c9\u03bd \u03bf\u03c0\u03cc\u03c4\u03b5 \u03b8\u03b1 \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03b7 \u03ba\u03b1\u03c4\u03b1\u03bd\u03ac\u03bb\u03c9\u03c3\u03ae \u03c4\u03bf\u03c5 \u03bd\u03b1 \u03b3\u03af\u03bd\u03b5\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03c6\u03b5\u03b9\u03b4\u03ce, \u03ba\u03ac\u03c4\u03b9 \u03c0\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b3\u03b5\u03bd\u03b9\u03ba\u03cc\u03c2 \u03ba\u03b1\u03bd\u03cc\u03bd\u03b1\u03c2 \u03c3\u03c4\u03b7 \u03c3\u03c9\u03c3\u03c4\u03ae \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae, \u03b5\u03be\u03ac\u03bb\u03bb\u03bf\u03c5 \u03b4\u03b5\u03bd \u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03c5\u03c0\u03b5\u03c1\u03b2\u03bf\u03bb\u03ad\u03c2 \u03c3\u03c4\u03bf \u03c4\u03c1\u03b1\u03c0\u03ad\u03b6\u03b9 \u03b1\u03bb\u03bb\u03ac \u03c0\u03bf\u03b9\u03ba\u03b9\u03bb\u03af\u03b1.<br />\n<br />\n<b>\u0393\u03b9\u03b1\u03bf\u03cd\u03c1\u03c4\u03b9</b><br />\n\u03a0\u03bb\u03bf\u03cd\u03c3\u03b9\u03bf \u03c3\u03b5 \u03c0\u03c1\u03c9\u03c4\u03b5\u0390\u03bd\u03b5\u03c2, \u03b5\u03af\u03bd\u03b1\u03b9 \u03b9\u03b4\u03b1\u03bd\u03b9\u03ba\u03ae \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03b3\u03b9\u03b1 \u03c4\u03bf \u03be\u03b5\u03ba\u03af\u03bd\u03b7\u03bc\u03b1 \u03c4\u03b7\u03c2 \u03b7\u03bc\u03ad\u03c1\u03b1\u03c2 \u03c3\u03c5\u03bd\u03b4\u03c5\u03b1\u03c3\u03c4\u03b9\u03ba\u03ac \u03bc\u03b5 \u03be\u03b7\u03c1\u03bf\u03cd\u03c2 \u03ba\u03b1\u03c1\u03c0\u03bf\u03cd\u03c2, \u03b2\u03c1\u03ce\u03bc\u03b7, \u03c6\u03c1\u03bf\u03cd\u03c4\u03b1, \u03c3\u03c0\u03cc\u03c1\u03bf\u03c5\u03c2 \u03c4\u03c3\u03af\u03b1 \u03ba\u03b1\u03b9 \u03bc\u03ad\u03bb\u03b9 (\u03ae \u03b5\u03bd\u03b1\u03bb\u03bb\u03b1\u03ba\u03c4\u03b9\u03ba\u03ac \u03c3\u03b9\u03c1\u03cc\u03c0\u03b9 \u03b1\u03b3\u03b1\u03cd\u03b7\u03c2) \u03b1\u03bb\u03bb\u03ac \u03ba\u03b1\u03b9 \u03c9\u03c2 \u03b5\u03bd\u03b4\u03b9\u03ac\u03bc\u03b5\u03c3\u03bf \u03b3\u03b5\u03cd\u03bc\u03b1 \u03c0\u03c1\u03b9\u03bd \u03ae \u03bc\u03b5\u03c4\u03ac \u03c4\u03b7\u03bd \u03c0\u03c1\u03bf\u03c0\u03cc\u03bd\u03b7\u03c3\u03b7. \u0395\u03af\u03bd\u03b1\u03b9 \u03c0\u03bb\u03bf\u03cd\u03c3\u03b9\u03bf \u03c3\u03b5 \u03b1\u03c3\u03b2\u03ad\u03c3\u03c4\u03b9\u03bf \u03c0\u03bf\u03c5 \u03b5\u03bd\u03b4\u03c5\u03bd\u03b1\u03bc\u03ce\u03bd\u03b5\u03b9 \u03c4\u03b1 \u03bf\u03c3\u03c4\u03ac \u03ba\u03b1\u03b9 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b1\u03c0\u03b1\u03c1\u03b1\u03af\u03c4\u03b7\u03c4\u03bf \u03ba\u03b1\u03b9 \u03c3\u03c4\u03bf\u03bd \u03b1\u03bd\u03b4\u03c1\u03b9\u03ba\u03cc \u03bf\u03c1\u03b3\u03b1\u03bd\u03b9\u03c3\u03bc\u03cc. \u03a3\u03b7\u03bc\u03b5\u03b9\u03ce\u03bd\u03bf\u03c5\u03bc\u03b5 \u03b3\u03b5\u03bd\u03b9\u03ba\u03ac \u03cc\u03c4\u03b9 \u03c4\u03c1\u03bf\u03c6\u03ad\u03c2 \u03c0\u03bb\u03bf\u03cd\u03c3\u03b9\u03b5\u03c2 \u03c3\u03b5 \u03c6\u03c5\u03c4\u03b9\u03ba\u03ad\u03c2 \u03af\u03bd\u03b5\u03c2 (\u03cc\u03c0\u03c9\u03c2 \u03b7 \u03b2\u03c1\u03ce\u03bc\u03b7 \u03ba\u03b1\u03b9 \u03c4\u03b1 \u03c6\u03c1\u03bf\u03cd\u03c4\u03b1 \u03c0\u03bf\u03c5 \u03c0\u03c1\u03bf\u03b1\u03bd\u03b1\u03c6\u03ad\u03c1\u03b1\u03bc\u03b5) \u03b5\u03af\u03bd\u03b1\u03b9 \u03c7\u03bf\u03c1\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03ad\u03c2 \u03ba\u03b1\u03b9 \u03b2\u03bf\u03b7\u03b8\u03bf\u03cd\u03bd \u03c3\u03c4\u03b7 \u03bc\u03b5\u03af\u03c9\u03c3\u03b7 \u03c4\u03c9\u03bd \u03b5\u03c0\u03b9\u03c0\u03ad\u03b4\u03c9\u03bd \u03c4\u03b7\u03c2 &#171;\u03ba\u03b1\u03ba\u03ae\u03c2&#187; (LDL) \u03c7\u03bf\u03bb\u03b7\u03c3\u03c4\u03b5\u03c1\u03af\u03bd\u03b7\u03c2 \u03bf\u03c0\u03cc\u03c4\u03b5 \u03c4\u03b1 \u03bf\u03c6\u03ad\u03bb\u03b7 \u03c4\u03bf\u03c5\u03c2 \u03b5\u03bd\u03b9\u03c3\u03c7\u03cd\u03bf\u03bd\u03c4\u03b1\u03b9 \u03cc\u03c4\u03b1\u03bd \u03c3\u03c5\u03bd\u03b4\u03c5\u03ac\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03ba\u03b1\u03c4\u03ac\u03bb\u03bb\u03b7\u03bb\u03b5\u03c2 \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ad\u03c2 \u03cc\u03c0\u03c9\u03c2 \u03c4\u03bf \u03b3\u03b9\u03b1\u03bf\u03cd\u03c1\u03c4\u03b9 \u03ba\u03b1\u03b9 \u03c4\u03bf \u03b3\u03ac\u03bb\u03b1 (\u03ba\u03b1\u03c4\u03ac \u03c0\u03c1\u03bf\u03c4\u03af\u03bc\u03b7\u03c3\u03b7 \u03b1\u03c0\u03bf\u03b2\u03bf\u03c5\u03c4\u03c5\u03c1\u03c9\u03bc\u03ad\u03bd\u03bf \u03ae \u03b1\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03bb\u03ae\u03c1\u03b5\u03c2, \u03c3\u03b5 \u03bc\u03b9\u03ba\u03c1\u03cc\u03c4\u03b5\u03c1\u03b7 \u03c0\u03bf\u03c3\u03cc\u03c4\u03b7\u03c4\u03b1).<br />\n<br />\n<b>\u03a6\u03c1\u03bf\u03cd\u03c4\u03b1</b><br />\n\u039f \u03ba\u03b1\u03bd\u03cc\u03bd\u03b1\u03c2 \u03c0\u03bf\u03c5 \u03b9\u03c3\u03c7\u03cd\u03b5\u03b9 \u03b3\u03b9\u03b1 \u03c4\u03b7 \u03b3\u03c5\u03bd\u03b1\u03b9\u03ba\u03b5\u03af\u03b1 \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae \u03b9\u03c3\u03c7\u03cd\u03b5\u03b9 \u03ba\u03b1\u03b9 \u03b3\u03b9\u03b1 \u03c4\u03b7\u03bd \u03b1\u03bd\u03c4\u03c1\u03b9\u03ba\u03ae: \u03a4\u03b1 \u03c6\u03c1\u03bf\u03cd\u03c4\u03b1 \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b1\u03bd\u03b1\u03c0\u03cc\u03c3\u03c0\u03b1\u03c3\u03c4\u03bf \u03ba\u03bf\u03bc\u03bc\u03ac\u03c4\u03b9 \u03c4\u03bf\u03c5 \u03ba\u03b1\u03b8\u03b7\u03bc\u03b5\u03c1\u03b9\u03bd\u03bf\u03cd \u03b4\u03b9\u03b1\u03b9\u03c4\u03bf\u03bb\u03bf\u03b3\u03af\u03bf\u03c5 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03b1\u03c0\u03bf\u03bb\u03b1\u03bc\u03b2\u03ac\u03bd\u03bf\u03c5\u03bc\u03b5 \u03c4\u03b1 \u03bf\u03c6\u03ad\u03bb\u03b7 \u03c4\u03bf\u03c5\u03c2 \u03c3\u03c4\u03bf \u03ad\u03c0\u03b1\u03ba\u03c1\u03bf. \u0391\u03bd\u03b1\u03c6\u03ad\u03c1\u03bf\u03c5\u03bc\u03b5 \u03c0\u03b9\u03bf \u03c3\u03c5\u03b3\u03ba\u03b5\u03ba\u03c1\u03b9\u03bc\u03ad\u03bd\u03b1 \u03c4\u03b7 \u03b2\u03b9\u03c4\u03b1\u03bc\u03af\u03bd\u03b7 C \u03c0\u03bf\u03c5 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03ac\u03bb\u03bb\u03c9\u03bd, \u03c3\u03c5\u03c3\u03c7\u03b5\u03c4\u03af\u03b6\u03b5\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03c4\u03b7\u03bd \u03ba\u03b1\u03bb\u03ae \u03c5\u03b3\u03b5\u03af\u03b1 \u03c4\u03bf\u03c5 \u03c0\u03c1\u03bf\u03c3\u03c4\u03ac\u03c4\u03b7.<br />\n<br />\n<b>\u03a0\u03bf\u03bb\u03cd\u03c3\u03c0\u03bf\u03c1\u03bf \u03c8\u03c9\u03bc\u03af</b><br />\n\u03a4\u03b1 \u03c0\u03c1\u03bf\u03ca\u03cc\u03bd\u03c4\u03b1 \u03c0\u03bf\u03c5 \u03c0\u03b1\u03c1\u03b1\u03c3\u03ba\u03b5\u03c5\u03ac\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03b1\u03bb\u03b5\u03cd\u03c1\u03b9 \u03bf\u03bb\u03b9\u03ba\u03ae\u03c2 \u03ac\u03bb\u03b5\u03c3\u03b7\u03c2 \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03b9\u03bf \u03c5\u03b3\u03b9\u03b5\u03b9\u03bd\u03ae \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03b1\u03c0\u03cc \u03b1\u03c5\u03c4\u03ac \u03c0\u03bf\u03c5 \u03c0\u03b1\u03c1\u03ac\u03b3\u03bf\u03bd\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03bb\u03b5\u03c5\u03ba\u03cc \u03b1\u03bb\u03b5\u03cd\u03c1\u03b9. \u0391\u03bd\u03b1\u03c6\u03bf\u03c1\u03b9\u03ba\u03ac \u03bc\u03b5 \u03c4\u03bf \u03c8\u03c9\u03bc\u03af, \u03c4\u03bf \u03c0\u03bf\u03bb\u03cd\u03c3\u03c0\u03bf\u03c1\u03bf \u03bc\u03c0\u03bf\u03c1\u03b5\u03af \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b5\u03bb\u03b1\u03c6\u03c1\u03ce\u03c2 \u03c0\u03b9\u03bf \u03b8\u03b5\u03c1\u03bc\u03b9\u03b4\u03bf\u03b3\u03cc\u03bd\u03bf \u03b1\u03c0\u03cc \u03c4\u03bf \u03c3\u03c5\u03bc\u03b2\u03b1\u03c4\u03b9\u03ba\u03cc \u03bb\u03cc\u03b3\u03c9 \u03c4\u03c9\u03bd \u03c0\u03bf\u03bb\u03bb\u03ce\u03bd \u03b5\u03b9\u03b4\u03ce\u03bd \u03ba\u03b1\u03c1\u03c0\u03bf\u03cd \u03c0\u03bf\u03c5 \u03c0\u03b5\u03c1\u03b9\u03ad\u03c7\u03b5\u03b9, \u03c9\u03c3\u03c4\u03cc\u03c3\u03bf \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03b9\u03bf \u03c7\u03bf\u03c1\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc \u03b5\u03bd\u03ce \u03b5\u03bd\u03c4\u03b1\u03b3\u03bc\u03ad\u03bd\u03bf \u03c3\u03b5 \u03ad\u03bd\u03b1 \u03b9\u03c3\u03bf\u03c1\u03c1\u03bf\u03c0\u03b7\u03bc\u03ad\u03bd\u03bf \u03b4\u03b9\u03b1\u03b9\u03c4\u03bf\u03bb\u03cc\u03b3\u03b9\u03bf \u03bc\u03c0\u03bf\u03c1\u03b5\u03af \u03bd\u03b1 \u03b2\u03bf\u03b7\u03b8\u03ae\u03c3\u03b5\u03b9 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03ac\u03bb\u03bb\u03c9\u03bd \u03ba\u03b1\u03b9 \u03c3\u03c4\u03b7\u03bd \u03ba\u03b1\u03bb\u03ae \u03bb\u03b5\u03b9\u03c4\u03bf\u03c5\u03c1\u03b3\u03af\u03b1 \u03c4\u03b7\u03c2 \u03ba\u03b1\u03c1\u03b4\u03b9\u03ac\u03c2. \u039c\u03b9\u03b1 \u03ba\u03b1\u03bb\u03ae \u03c0\u03c1\u03cc\u03c4\u03b1\u03c3\u03b7 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b5\u03af\u03bd\u03b1\u03b9 \u03bd\u03b1 \u03c4\u03bf \u03c3\u03c5\u03bd\u03b4\u03c5\u03ac\u03c3\u03b5\u03c4\u03b5 \u03c3\u03c4\u03bf \u03c0\u03c1\u03c9\u03b9\u03bd\u03cc \u03c3\u03b1\u03c2 \u03b3\u03b5\u03cd\u03bc\u03b1 \u03bc\u03b5 \u03b2\u03bf\u03cd\u03c4\u03c5\u03c1\u03bf \u03b1\u03bc\u03c5\u03b3\u03b4\u03ac\u03bb\u03bf\u03c5 \u03c0\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03bb\u03bf\u03cd\u03c3\u03b9\u03bf \u03c3\u03b5 \u03bc\u03bf\u03bd\u03bf\u03b1\u03ba\u03cc\u03c1\u03b5\u03c3\u03c4\u03b1 \u03bb\u03b9\u03c0\u03b1\u03c1\u03ac \u03c4\u03b1 \u03bf\u03c0\u03bf\u03af\u03b1 \u03c3\u03c5\u03bd\u03b4\u03ad\u03bf\u03bd\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03c4\u03b7\u03bd \u03c0\u03c1\u03cc\u03bb\u03b7\u03c8\u03b7 \u03c4\u03c9\u03bd \u03ba\u03b1\u03c1\u03b4\u03b9\u03b1\u03b3\u03b3\u03b5\u03b9\u03b1\u03ba\u03ce\u03bd \u03bd\u03bf\u03c3\u03b7\u03bc\u03ac\u03c4\u03c9\u03bd.<br />\n<br />\n<b>\u0391\u03b2\u03b3\u03ac \u03bc\u03b5 \u03bd\u03c4\u03bf\u03bc\u03ac\u03c4\u03b1</b><br />\n\u039e\u03b5\u03ba\u03b9\u03bd\u03ae\u03c3\u03c4\u03b5 \u03c4\u03b7 \u03bc\u03ad\u03c1\u03b1 \u03c3\u03b1\u03c2 \u03c6\u03c4\u03b9\u03ac\u03c7\u03bd\u03bf\u03bd\u03c4\u03b1\u03c2 \u03ad\u03bd\u03b1\u03bd \u03c9\u03c1\u03b1\u03af\u03bf \u03ba\u03b1\u03b3\u03b9\u03b1\u03bd\u03ac \u03bc\u03b5 2 \u03b1\u03b2\u03b3\u03ac. \u0395\u03af\u03bd\u03b1\u03b9, \u03c7\u03bf\u03c1\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc\u03c2, \u03c0\u03c1\u03c9\u03c4\u03b5\u03ca\u03bd\u03bf\u03cd\u03c7\u03bf\u03c2 \u03ba\u03b1\u03b9 \u03c5\u03b3\u03b9\u03b5\u03b9\u03bd\u03cc\u03c2. \u0393\u03b9\u03b1 \u03bd\u03b1 \u03bc\u03b7\u03bd \u03ad\u03c7\u03b5\u03c4\u03b5 \u03c0\u03bf\u03bb\u03bb\u03ac \u03c5\u03b3\u03c1\u03ac \u03b1\u03c0\u03cc \u03c4\u03b7\u03bd \u03bd\u03c4\u03bf\u03bc\u03ac\u03c4\u03b1, \u03c3\u03bf\u03c5\u03c1\u03ce\u03c3\u03c4\u03b5 \u03c4\u03b7\u03bd \u03b1\u03c6\u03bf\u03cd \u03c4\u03b7\u03bd \u03c4\u03c1\u03af\u03c8\u03b5\u03c4\u03b5 \u03b5\u03bd\u03ce \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03bc\u03b7\u03bd \u03c7\u03ac\u03c3\u03b5\u03b9 \u03c4\u03b1 \u03b8\u03c1\u03b5\u03c0\u03c4\u03b9\u03ba\u03ac \u03c3\u03c5\u03c3\u03c4\u03b1\u03c4\u03b9\u03ba\u03ac \u03c4\u03b7\u03c2 \u03c3\u03c4\u03bf \u03bc\u03b1\u03b3\u03b5\u03af\u03c1\u03b5\u03bc\u03b1, \u03bc\u03c0\u03bf\u03c1\u03b5\u03af\u03c4\u03b5 \u03bd\u03b1 \u03c4\u03b7\u03bd \u03c0\u03c1\u03bf\u03c3\u03b8\u03ad\u03c3\u03b5\u03c4\u03b5 \u03c3\u03c4\u03bf \u03c4\u03b7\u03b3\u03ac\u03bd\u03b9 \u03bc\u03b5 \u03c4\u03b1 \u03b1\u03b2\u03b3\u03ac \u03bb\u03af\u03b3\u03bf \u03c0\u03c1\u03b9\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03ad\u03c4\u03bf\u03b9\u03bc\u03b1. \u0395\u03b9\u03b4\u03b9\u03ba\u03ac \u03b3\u03b9\u03b1 \u03cc\u03c3\u03bf\u03c5\u03c2 \u03b3\u03c5\u03bc\u03bd\u03ac\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9, \u03bf\u03b9 \u03c0\u03c1\u03c9\u03c4\u03b5\u0390\u03bd\u03b5\u03c2 \u03c0\u03bf\u03c5 \u03c0\u03b5\u03c1\u03b9\u03ad\u03c7\u03bf\u03c5\u03bd \u03c4\u03b1 \u03b1\u03b2\u03b3\u03ac \u03b5\u03af\u03bd\u03b1\u03b9 \u03bf \u03b9\u03b4\u03b1\u03bd\u03b9\u03ba\u03cc\u03c2 \u03c4\u03c1\u03cc\u03c0\u03bf\u03c2 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03b1\u03bd\u03b1\u03b4\u03cc\u03bc\u03b7\u03c3\u03b7 \u03c4\u03c9\u03bd \u03bc\u03c5\u03ce\u03bd. \u0395\u03c0\u03af\u03c3\u03b7\u03c2, \u03b7 \u03bb\u03bf\u03c5\u03c4\u03b5\u0390\u03bd\u03b7 \u03ba\u03b1\u03b9 \u03b6\u03b5\u03b1\u03be\u03b1\u03bd\u03b8\u03af\u03bd\u03b7 \u03c0\u03bf\u03c5 \u03c0\u03b5\u03c1\u03b9\u03ad\u03c7\u03bf\u03c5\u03bd \u03c4\u03b1 \u03b1\u03b2\u03b3\u03ac \u03b2\u03bf\u03b7\u03b8\u03bf\u03cd\u03bd \u03c3\u03c4\u03b7 \u03bc\u03b5\u03af\u03c9\u03c3\u03b7 \u03c4\u03b7\u03c2 \u03c6\u03b8\u03bf\u03c1\u03ac\u03c2 \u03c4\u03c9\u03bd \u03bc\u03c5\u03ce\u03bd \u03b1\u03c0\u03cc \u03c4\u03b7 \u03b4\u03b9\u03b1\u03b4\u03b9\u03ba\u03b1\u03c3\u03af\u03b1 \u03c4\u03b7\u03c2 \u03b3\u03ae\u03c1\u03b1\u03bd\u03c3\u03b7\u03c2. \u03a3\u03b7\u03bc\u03b5\u03af\u03c9\u03c3\u03b7: \u03b9\u03b4\u03b1\u03bd\u03b9\u03ba\u03ac, \u03c0\u03c1\u03bf\u03c3\u03b8\u03ad\u03c3\u03c4\u03b5 \u03c0\u03ac\u03bd\u03c9 \u03b1\u03c0\u03cc \u03c4\u03b1 \u03b1\u03b2\u03b3\u03ac \u03b5\u03bb\u03ac\u03c7\u03b9\u03c3\u03c4\u03bf \u03b5\u03bb\u03b1\u03b9\u03cc\u03bb\u03b1\u03b4\u03bf \u03b5\u03bd\u03ce \u03b1\u03bd \u03c4\u03b1 \u03c6\u03c4\u03b9\u03ac\u03be\u03b5\u03c4\u03b5 \u03b3\u03b9\u03b1 \u03ba\u03c5\u03c1\u03af\u03c9\u03c2 \u03b3\u03b5\u03cd\u03bc\u03b1 \u03bc\u03c0\u03bf\u03c1\u03b5\u03af\u03c4\u03b5 \u03bd\u03b1 \u03c0\u03c1\u03bf\u03c3\u03b8\u03ad\u03c3\u03b5\u03c4\u03b5 \u03c3\u03c4\u03b7 \u03c3\u03c5\u03bd\u03c4\u03b1\u03b3\u03ae \u03c3\u03b1\u03c2 \u03bb\u03af\u03b3\u03bf \u03c4\u03c1\u03b1\u03b3\u03b1\u03bd\u03cc \u03c0\u03c1\u03bf\u03c3\u03bf\u03cd\u03c4\u03bf \u03ae \u03ba\u03b5\u03c1\u03ba\u03c5\u03c1\u03b1\u03ca\u03ba\u03cc \u03bd\u03bf\u03cd\u03bc\u03c0\u03bf\u03c5\u03bb\u03bf. \u038c\u03bb\u03b1 \u03cc\u03bc\u03c9\u03c2 \u03bc\u03b5 \u03c6\u03b5\u03b9\u03b4\u03ce.<br />\n<script async=\"\" src=\"//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js\"></script>\n\n<ins class=\"adsbygoogle\" data-ad-client=\"ca-pub-3023702475348982\" data-ad-format=\"fluid\" data-ad-layout-key=\"-e9+4z+b1-h3+6d\" data-ad-slot=\"3213945841\" style=\"display: block;\"></ins><script>\n     (adsbygoogle = window.adsbygoogle || []).push({});\n</script>\n\n<br />\n<b>\u03a3\u03b1\u03bb\u03ac\u03c4\u03b1 \u03bc\u03b5 \u03c8\u03b7\u03c4\u03cc \u03ba\u03bf\u03c4\u03cc\u03c0\u03bf\u03c5\u03bb\u03bf</b><br />\n\u03a3\u03c4\u03b7\u03bd \u03b1\u03c0\u03ce\u03bb\u03b5\u03b9\u03b1 \u03b2\u03ac\u03c1\u03bf\u03c5\u03c2 \u03bc\u03c0\u03bf\u03c1\u03b5\u03af \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b5\u03be\u03b1\u03b9\u03c1\u03b5\u03c4\u03b9\u03ba\u03cc\u03c2 \u03c3\u03cd\u03bc\u03bc\u03b1\u03c7\u03bf\u03c2 \u03bc\u03b9\u03b1 \u03c3\u03b1\u03bb\u03ac\u03c4\u03b1 \u03bc\u03b5 \u03ba\u03ac\u03b8\u03b5 \u03bb\u03bf\u03b3\u03ae\u03c2 \u03bb\u03b1\u03c7\u03b1\u03bd\u03b9\u03ba\u03ac \u03ba\u03b1\u03b9 \u03c0\u03c1\u03b1\u03c3\u03b9\u03bd\u03ac\u03b4\u03b5\u03c2 \u03b1\u03c6\u03bf\u03cd \u03b5\u03af\u03bd\u03b1\u03b9 \u03b5\u03c5\u03ad\u03bb\u03b9\u03ba\u03c4\u03b7, \u03b8\u03c1\u03b5\u03c0\u03c4\u03b9\u03ba\u03ae -\u03ba\u03b1\u03b9 \u03b1\u03bd\u03c4\u03b9\u03bf\u03be\u03b5\u03b9\u03b4\u03c9\u03c4\u03b9\u03ba\u03ae- \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03b5\u03bd\u03ce \u03bc\u03b5 \u03c4\u03b7\u03bd \u03c0\u03c1\u03bf\u03c3\u03b8\u03ae\u03ba\u03b7 \u03c8\u03b7\u03c4\u03bf\u03cd \u03ba\u03bf\u03c4\u03cc\u03c0\u03bf\u03c5\u03bb\u03bf\u03c5 \u03bc\u03b5\u03c4\u03b1\u03c4\u03c1\u03ad\u03c0\u03b5\u03c4\u03b1\u03b9 \u03c3\u03b5 \u03b5\u03bb\u03b1\u03c6\u03c1\u03cd \u03b3\u03b5\u03cd\u03bc\u03b1. \u0394\u03b5\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03c4\u03c5\u03c7\u03b1\u03af\u03bf \u03c4\u03bf \u03cc\u03c4\u03b9 \u03bf\u03b9 \u03c3\u03b1\u03bb\u03ac\u03c4\u03b5\u03c2 \u03b1\u03bd\u03ae\u03ba\u03bf\u03c5\u03bd \u03c3\u03c4\u03b9\u03c2 \u03b2\u03b1\u03c3\u03b9\u03ba\u03ad\u03c2 \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ad\u03c2 \u03ba\u03ac\u03b8\u03b5 \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03b9\u03ba\u03bf\u03cd \u03c0\u03c1\u03bf\u03b3\u03c1\u03ac\u03bc\u03bc\u03b1\u03c4\u03bf\u03c2 \u03b1\u03c6\u03bf\u03cd \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03ac\u03bb\u03bb\u03c9\u03bd \u03c3\u03c5\u03bc\u03b2\u03ac\u03bb\u03bb\u03bf\u03c5\u03bd \u03ba\u03b1\u03b9 \u03c3\u03c4\u03b7 \u03b4\u03b9\u03b1\u03c4\u03ae\u03c1\u03b7\u03c3\u03b7 \u03c4\u03b7\u03c2 \u03ba\u03b1\u03bb\u03ae\u03c2 \u03ba\u03b1\u03c4\u03ac\u03c3\u03c4\u03b1\u03c3\u03b7\u03c2 \u03c4\u03bf\u03c5 \u03b1\u03bd\u03bf\u03c3\u03bf\u03c0\u03bf\u03b9\u03b7\u03c4\u03b9\u03ba\u03bf\u03cd \u03c3\u03c5\u03c3\u03c4\u03ae\u03bc\u03b1\u03c4\u03bf\u03c2. \u03a9\u03c3\u03c4\u03cc\u03c3\u03bf, \u03b8\u03b1 \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03bd\u03b1 \u03b5\u03af\u03c3\u03c4\u03b5 \u03c0\u03c1\u03bf\u03c3\u03b5\u03ba\u03c4\u03b9\u03ba\u03bf\u03af \u03bc\u03b5 \u03c4\u03bf \u03bd\u03c4\u03c1\u03ad\u03c3\u03b9\u03bd\u03b3\u03ba. \u039c\u03b7\u03bd \u03ba\u03ac\u03bd\u03b5\u03c4\u03b5 \u03c5\u03c0\u03b5\u03c1\u03b2\u03bf\u03bb\u03ad\u03c2, \u03c0\u03c1\u03bf\u03c4\u03b5\u03af\u03bd\u03bf\u03c5\u03bc\u03b5 \u03bd\u03b1 \u03c0\u03c1\u03bf\u03c3\u03b8\u03ad\u03c3\u03b5\u03c4\u03b5 \u03b9\u03b4\u03b1\u03bd\u03b9\u03ba\u03ac \u03bc\u03b7\u03bb\u03cc\u03be\u03b9\u03b4\u03bf \u03b1\u03bd\u03b1\u03bc\u03b5\u03b9\u03b3\u03bc\u03ad\u03bd\u03bf \u03bc\u03b5 \u03b5\u03bb\u03b1\u03b9\u03cc\u03bb\u03b1\u03b4\u03bf \u03ba\u03b1\u03b9 \u03c6\u03c1\u03ad\u03c3\u03ba\u03b1 \u03bc\u03c5\u03c1\u03c9\u03b4\u03b9\u03ba\u03ac \u03c3\u03c4\u03b1 \u03bb\u03b1\u03c7\u03b1\u03bd\u03b9\u03ba\u03ac \u03c3\u03b1\u03c2 \u03b3\u03b9\u03b1 \u03bf\u03be\u03cd\u03c4\u03b7\u03c4\u03b1 \u03ba\u03b1\u03b9 \u03b5\u03c0\u03b9\u03c0\u03bb\u03ad\u03bf\u03bd \u03b3\u03b5\u03cd\u03c3\u03b7.<br />\n<br />\n<b>\u039c\u03c0\u03ad\u03c1\u03b3\u03ba\u03b5\u03c1 (\u03c6\u03c4\u03b9\u03b1\u03b3\u03bc\u03ad\u03bd\u03bf \u03c3\u03c9\u03c3\u03c4\u03ac \u03cc\u03bc\u03c9\u03c2!)</b><br />\n\u03a0\u03bf\u03b9\u03bf\u03c2 \u03bf \u03bb\u03cc\u03b3\u03bf\u03c2 \u03bd\u03b1 \u03c3\u03c4\u03b5\u03c1\u03b7\u03b8\u03b5\u03af\u03c4\u03b5 \u03c4\u03bf \u03b1\u03b3\u03b1\u03c0\u03b7\u03bc\u03ad\u03bd\u03bf \u03c3\u03b1\u03c2 \u03bc\u03c0\u03ad\u03c1\u03b3\u03ba\u03b5\u03c1 \u03cc\u03c4\u03b1\u03bd \u03b1\u03ba\u03bf\u03bb\u03bf\u03c5\u03b8\u03b5\u03af\u03c4\u03b5 \u03ad\u03bd\u03b1 \u03c0\u03b9\u03bf \u03c0\u03c1\u03bf\u03c3\u03b5\u03b3\u03bc\u03ad\u03bd\u03bf \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03b9\u03ba\u03cc \u03c0\u03bb\u03ac\u03bd\u03bf; \u0395\u03be\u03ac\u03bb\u03bb\u03bf\u03c5, \u03c3\u03c9\u03c3\u03c4\u03ae \u03b4\u03b9\u03b1\u03c4\u03c1\u03bf\u03c6\u03ae \u03c3\u03b7\u03bc\u03b1\u03af\u03bd\u03b5\u03b9 \u03c3\u03c9\u03c3\u03c4\u03ac \u03b5\u03ba\u03c4\u03b5\u03bb\u03b5\u03c3\u03bc\u03ad\u03bd\u03b5\u03c2 \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ad\u03c2. \u039a\u03ac\u03b8\u03b5 \u03c3\u03bd\u03b1\u03ba \u03ae \u03c0\u03b9\u03ac\u03c4\u03bf \u03bb\u03bf\u03b9\u03c0\u03cc\u03bd \u03c0\u03bf\u03c5 \u03c6\u03c4\u03b9\u03ac\u03c7\u03bd\u03b5\u03c4\u03b1\u03b9 \u03bc\u03b5 \u03c4\u03c1\u03cc\u03c0\u03bf \u03c0\u03bf\u03c5 \u03c4\u03bf \u03ba\u03b1\u03b8\u03b9\u03c3\u03c4\u03ac \u03c5\u03b3\u03b9\u03b5\u03b9\u03bd\u03cc \u03ba\u03b1\u03b9 \u03bd\u03cc\u03c3\u03c4\u03b9\u03bc\u03bf- \u03bc\u03b5 \u03b1\u03bd\u03ac\u03bb\u03bf\u03b3\u03b1 \u03b3\u03b5\u03c5\u03c3\u03c4\u03b9\u03ba\u03ac \u03c4\u03c1\u03b9\u03ba -\u03b4\u03b5\u03bd \u03b5\u03af\u03bd\u03b1\u03b9 \u03b1\u03c0\u03b1\u03b3\u03bf\u03c1\u03b5\u03c5\u03c4\u03b9\u03ba\u03cc. \u039f \u03ba\u03b1\u03bd\u03cc\u03bd\u03b1\u03c2 \u03b9\u03c3\u03c7\u03cd\u03b5\u03b9 \u03ba\u03b1\u03b9 \u03b3\u03b9\u03b1 \u03c4\u03bf \u03bc\u03c0\u03ad\u03c1\u03b3\u03ba\u03b5\u03c1, \u03c0\u03bf\u03c5 \u03b1\u03bd \u03c4\u03bf \u03c6\u03c4\u03b9\u03ac\u03be\u03b5\u03c4\u03b5 \u03bc\u03b5 \u03bc\u03c0\u03b9\u03c6\u03c4\u03ad\u03ba\u03b9 \u03b1\u03c0\u03cc \u03ac\u03c0\u03b1\u03c7\u03bf \u03bc\u03bf\u03c3\u03c7\u03b1\u03c1\u03af\u03c3\u03b9\u03bf \u03ba\u03b9\u03bc\u03ac, \u03ae \u03ba\u03b9\u03bc\u03ac \u03ba\u03bf\u03c4\u03cc\u03c0\u03bf\u03c5\u03bb\u03bf\u03c5 \u03ae \u03b3\u03b1\u03bb\u03bf\u03c0\u03bf\u03cd\u03bb\u03b1\u03c2, \u03c8\u03c9\u03bc\u03af \u03bf\u03bb\u03b9\u03ba\u03ae\u03c2 \u03ac\u03bb\u03b5\u03c3\u03b7\u03c2, \u03bd\u03c4\u03bf\u03bc\u03ac\u03c4\u03b1, \u03bc\u03b1\u03c1\u03bf\u03cd\u03bb\u03b9, \u03ba\u03c1\u03b5\u03bc\u03bc\u03cd\u03b4\u03b9 \u03ba\u03b1\u03b9 \u03bc\u03b9\u03b1 \u03b5\u03bb\u03b1\u03c6\u03c1\u03b9\u03ac \u03c3\u03ac\u03bb\u03c4\u03c3\u03b1 \u03b3\u03b9\u03b1\u03bf\u03c5\u03c1\u03c4\u03b9\u03bf\u03cd \u03b1\u03bd\u03c4\u03af \u03b3\u03b9\u03b1 \u03bc\u03b1\u03b3\u03b9\u03bf\u03bd\u03ad\u03b6\u03b1 \u03c9\u03c2 \u03b2\u03ac\u03c3\u03b7, \u03b5\u03af\u03bd\u03b1\u03b9 \u03c3\u03b1\u03c6\u03ce\u03c2 \u03ba\u03b1\u03bb\u03cd\u03c4\u03b5\u03c1\u03bf \u03b1\u03c0\u03cc \u03c4\u03b7\u03bd \u03c3\u03c5\u03bd\u03b7\u03b8\u03b9\u03c3\u03bc\u03ad\u03bd\u03b7 \u03ac\u03ba\u03c1\u03c9\u03c2 \u03c0\u03bb\u03b7\u03b8\u03c9\u03c1\u03b9\u03ba\u03ae \u03b5\u03ba\u03b4\u03bf\u03c7\u03ae \u03c4\u03bf\u03c5&#8230; \u039c\u03c0\u03bf\u03c1\u03b5\u03af\u03c4\u03b5 \u03b5\u03c0\u03af\u03c3\u03b7\u03c2 \u03bd\u03b1 \u03c0\u03b1\u03bd\u03ac\u03c1\u03b5\u03c4\u03b5 \u03c6\u03b9\u03bb\u03ad\u03c4\u03bf \u03ba\u03bf\u03c4\u03cc\u03c0\u03bf\u03c5\u03bb\u03bf\u03c5 \u03c3\u03b5 \u03ad\u03bd\u03b1 \u03bc\u03af\u03b3\u03bc\u03b1 \u03bc\u03b5 \u03bc\u03c5\u03c1\u03c9\u03b4\u03b9\u03ba\u03ac-\u03ae \u03b1\u03c0\u03bb\u03ac \u03c3\u03b5 \u03b3\u03b1\u03bb\u03ad\u03c4\u03b1- \u03ba\u03b1\u03b9 \u03bd\u03b1 \u03c4\u03bf \u03c8\u03ae\u03c3\u03b5\u03c4\u03b5 \u03c3\u03c4\u03bf \u03c6\u03bf\u03cd\u03c1\u03bd\u03bf. \u03a4\u03bf \u03ba\u03cc\u03ba\u03ba\u03b9\u03bd\u03bf \u03ba\u03c1\u03ad\u03b1\u03c2 \u03ba\u03b1\u03b9 \u03c4\u03bf \u03ba\u03bf\u03c4\u03cc\u03c0\u03bf\u03c5\u03bb\u03bf \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03bf\u03bb\u03cd \u03ba\u03b1\u03bb\u03ad\u03c2 \u03c0\u03b7\u03b3\u03ad\u03c2 \u03c0\u03c1\u03c9\u03c4\u03b5\u0390\u03bd\u03b7\u03c2 \u03c0\u03bf\u03c5 \u03b2\u03bf\u03b7\u03b8\u03ac \u03c3\u03c4\u03b7\u03bd \u03ba\u03b1\u03cd\u03c3\u03b7 \u03bb\u03af\u03c0\u03bf\u03c5\u03c2 \u03ba\u03b1\u03b9 \u03b4\u03af\u03bd\u03b5\u03b9 \u03b5\u03bd\u03ad\u03c1\u03b3\u03b5\u03b9\u03b1 \u03b5\u03bd\u03ce \u03b7 \u03bd\u03c4\u03bf\u03bc\u03ac\u03c4\u03b1 \u03c0\u03b5\u03c1\u03b9\u03ad\u03c7\u03b5\u03b9 \u03bb\u03c5\u03ba\u03bf\u03c0\u03ad\u03bd\u03b9\u03bf \u03c0\u03bf\u03c5 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03ac\u03bb\u03bb\u03c9\u03bd \u03c3\u03c5\u03bc\u03b2\u03ac\u03bb\u03bb\u03b5\u03b9 \u03c3\u03c4\u03b7 \u03b4\u03b9\u03b1\u03c4\u03ae\u03c1\u03b7\u03c3\u03b7 \u03c4\u03b7\u03c2 \u03c5\u03b3\u03b5\u03af\u03b1\u03c2 \u03c4\u03bf\u03c5 \u03c0\u03c1\u03bf\u03c3\u03c4\u03ac\u03c4\u03b7.<br />\n<br />\n<a href=\"https://www.olivemagazine.gr/%CF%87%CF%81%CE%AE%CF%83%CE%B9%CE%BC%CE%B1/%CE%B4%CE%B9%CE%B1%CF%84%CF%81%CE%BF%CF%86%CE%B9%CE%BA%CE%AC-%CF%87%CF%81%CE%AE%CF%83%CE%B9%CE%BC%CE%B1/%CF%84%CE%B9-%CF%80%CF%81%CE%AD%CF%80%CE%B5%CE%B9-%CE%BD%CE%B1-%CF%84%CF%81%CF%8E%CE%BD%CE%B5-%CE%BF%CE%B9-%CE%AC%CE%BD%CF%84%CF%81%CE%B5%CF%82-%CE%B3%CE%B9%CE%B1-%CE%BD%CE%B1-%CF%87%CE%AC%CF%83%CE%BF/\" target=\"_blank\">\u03a0\u03b7\u03b3\u03ae</a><br />\n<div>\n<br /></div>\n</div>\n<!-- Composite Start -->\n\n<br />\n<div id=\"M176588ScriptRootC130354\">\n<div id=\"M176588PreloadC130354\">\nLoading...\n    </div>\n<script>\n                (function(){\n            var D=new Date(),d=document,b='body',ce='createElement',ac='appendChild',st='style',ds='display',n='none',gi='getElementById';\n            var i=d[ce]('iframe');i[st][ds]=n;d[gi](\"M176588ScriptRootC130354\")[ac](i);try{var iw=i.contentWindow.document;iw.open();iw.writeln(\"<ht\"+\"ml><bo\"+\"dy></bo\"+\"dy></ht\"+\"ml>\");iw.close();var c=iw[b];}\n            catch(e){var iw=d;var c=d[gi](\"M176588ScriptRootC130354\");}var dv=iw[ce]('div');dv.id=\"MG_ID\";dv[st][ds]=n;dv.innerHTML=130354;c[ac](dv);\n            var s=iw[ce]('script');s.async='async';s.defer='defer';s.charset='utf-8';s.src=\"//jsc.mgid.com/a/s/astrapinews.gr.1