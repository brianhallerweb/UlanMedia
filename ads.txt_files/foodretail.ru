gin\">\n  <input type='hidden' name='ref' value='%2Fads.txt'>\n  <fieldset>\n    <a href=\"#\" class=\"closeModal simplemodal-close\">\u0417\u0430\u043a\u0440\u044b\u0442\u044c<i><!-- --></i></a>\n    <p class=\"title-h3\">\u0412\u0445\u043e\u0434 \u043d\u0430 \u0441\u0430\u0439\u0442</p>\n    <p class=\"f-lbl-inp i-text\">\n      <label for=\"u-login\"><strong>\u042d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u0430\u044f  \u043f\u043e\u0447\u0442\u0430</strong></label>\n      <input type=\"text\" name=\"login\" id=\"u-login\" tabindex=\"1\" value=\"\" />\n    </p>\n    <p class=\"f-lbl-inp i-text\">\n      <label for=\"u-pass\" class=\"all-width\">\n        <strong>\u041f\u0430\u0440\u043e\u043b\u044c</strong>\n        <a href=\"/signup/remind\">\u042f \u043f\u043e\u0434\u0437\u0430\u0431\u044b\u043b \u043f\u0430\u0440\u043e\u043b\u044c</a>\n        <em><!-- --></em>\n      </label>\n      <input type=\"password\" name=\"password\" id=\"u-pass\" tabindex=\"2\" value=\"\" />\n    </p>\n    <div class=\"f-wrap all-width\">\n      <p class=\"f-inp-lbl i-checkbox\">\n        <input type=\"checkbox\" name=\"remember\" id=\"u-rem\" value=\"1\" checked=\"checked\" />\n        <label for=\"u-rem\">\u0417\u0430\u043f\u043e\u043c\u043d\u0438\u0442\u0435 \u043c\u0435\u043d\u044f</label>\n      </p>\n      <p class=\"i-submit\">\n        <input type=\"submit\" value=\"\u0412\u043e\u0439\u0442\u0438\" tabindex=\"3\" />\n      </p>\n      <em><!-- --></em>\n    </div>\n    <p class=\"f-text\">\n      <a href=\"/signup\">\u0420\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044f</a>\n    </p>\n  </fieldset>\n</form>\n\n<script type=\"text/javascript\">\n  $(document).ready(function() {\n    var form = $(\"form#form-modal-login\");\n\n    function showAuthModal(offset) {\n      form.validate({\n        rules: {\n          \"login\": \"required\",\n          \"password\": \"required\"\n        },\n        messages: {\n          \"login\": \"\u0412\u0432\u0435\u0434\u04