m.id;\r\n\r\n        if (rttrShowedBanners >= rttrTotalBanners) {\r\n          // \u041a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u043e\u043a\u0430\u0437\u0430\u043d\u043d\u044b\u0445 \u0431\u0430\u043d\u043d\u0435\u0440\u043e\u0432 \u0431\u043e\u043b\u044c\u0448\u0435 \u0438\u043b\u0438 \u0440\u0430\u0432\u043d\u043e \u043e\u0431\u0449\u0435\u043c\u0443 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0443 \u0431\u0430\u043d\u043d\u0435\u0440\u043e\u0432. \u041e\u0431\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u0435 \u0434\u0430\u043b\u0435\u0435 \u043d\u0435\u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e.\r\n          clearInterval(rttrMainInterval);\r\n\r\n          // \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0437\u0430\u0433\u043b\u0443\u0448\u043a\u0443 \u043d\u0430 \u043c\u0435\u0441\u0442\u0435 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0433\u043e \u043e\u0431\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u044f\r\n          /*$('#rttr_bnr_' + oldBannerId).fadeOut(900,function(){\r\n           $(this).attr('src', '/rotatorfiles/empty/' + type + '/iframe.html' ).fadeIn(900);\r\n           });*/\r\n          return;\r\n        }\r\n\r\n        $.get(\"http://foodretail.ru/rotatorstat/next?type=\" + type + '&page=' + rttrCurrentPage + '&ids=' + bannersOnPage.join(','), function(info) {\r\n          if (!info.error) {\r\n            var iframe = $('#rttr_bnr_' + oldBannerId);\r\n\r\n            var parent = iframe.parent();\r\n\r\n            // \u0421\u043c\u0435\u043d\u0430 \u0431\u0430\u043d\u043d\u0435\u0440\u0430\r\n            parent.fadeOut(900,function(){\r\n              // \u0417\u0430\u043c\u0435\u043d\u0430 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u043e\u0432 \u0444\u0440\u0435\u0439\u043c\u0430 \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u043d\u043e\u0432\u043e\u043c\u0443 \u0431\u0430\u043d\u043d\u0435\u0440\u0443\r\n              var newFrame = document.createElement(\"iframe\");\r\n              newFrame.src = info.src;\r\n              newFrame.dataset.id = info.id;\r\n              newFrame.dataset.ri = info.ri;\r\n              newFrame.id = 'rttr_bnr_' + info.id;\r\n              newFrame.className = 'rttr_bnr';\r\n              newFrame.style.width = iframe.width() + 'px';\r\n              newFrame.style.height = iframe.height() + 'px';\r\n              newFrame.frameBorder = '0';\r\n              newFrame.scrolling = 'no';\r\n\r\n              iframe.replaceWith(newFrame);\r\n\r\n              $(this).fadeIn(900);\r\n\r\n              var oldIdIndex = bannersOnPage.indexOf(oldBannerId);\r\n              if (-1 !== oldIdIndex) {\r\n                // \u0423\u0434\u0430\u043b\u0435\u043d\u0438\u0435 \u0441\u043e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0441\u0442\u0430\u0440\u043e\u0433\u043e \u0431\u0430\u043d\u043d\u0435\u0440\u0430\r\n                bannersOnPage.splice(oldIdIndex, 1);\r\n              }\r\n\r\n              var newIdIndex = bannersOnPage.indexOf(info.id);\r\n              if (-1 === newIdIndex) {\r\n                // \u0414\u043e\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0443 \u043d\u043e\u0432\u043e\u0433\u043e \u0431\u0430\u043d\u043d\u0435\u0440\u0430\r\n                bannersOnPage.push(info.id);\r\n              }\r\n\r\n              setTimeout(rttrAddToQueue, info.ri * 1000, info.id, type);\r\n\r\n              rttrShowedBanners += 1;\r\n\r\n              reloadInProgress = false;\r\n            });\r\n          }\r\n        }, \"json\");\r\n      }\r\n\r\n      var bannerIframes = $('.rttr_bnr');\r\n      var bannersOnPage = [];\r\n      var rttrCurrentPage = '';\r\n\r\n      if (bannerIframes.length) {\r\n        bannerIframes.each(function(ind, iframe) {\r\n          var sec = $(iframe).data('ri');\r\n          var id = $(iframe).data('id');\r\n          var type = $(iframe).data('type');\r\n\r\n          bannersOnPage.push(id);\r\n\r\n          if (!rttrTotalBanners) {\r\n            rttrTotalBanners = $(iframe).data('total');\r\n          }\r\n          rttrShowedBanners += 1;\r\n\r\n          if (!rttrCurrentPage) {\r\n            rttrCurrentPage = $(iframe).data('page');\r\n          }\r\n\r\n          setTimeout(rttrAddToQueue, sec * 1000, id, type);\r\n        });\r\n      }\r\n    }\r\n    /**\r\n     * \u0414\u043e\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0431\u0430\u043d\u043d\u0435\u0440\u043e\u0432 \u0441\u043f\u0440\u0430\u0432\u0430 \u0432 \u0431\u043b\u043e\u043a\u0435 .col3\r\n     * \u0432\u043e \u0432\u0441\u044e \u0432\u044b\u0441\u043e\u0442\u0443 \u0441\u0430\u0439\u0442\u0430 \u043d\u043e \u043d\u0435 \u0431\u043e\u043b\u044c\u0448\u0435 10\r\n     * \u041e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u0435.\r\n     * 1. \u041a\u043b\u0430\u0441\u0441 \u043a\u043e\u043d\u0442\u0435\u043d\u0442\u0430 - .container-wrap\r\n     * 2. \u041a\u043b\u0430\u0441\u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u0441\u0430\u0439\u0434\u0431\u0430\u0440\u0430 - .col3\r\n     * 3. \u041c\u0435\u0441\u0442\u043e \u043a\u0443\u0434\u0430 \u0432\u0441\u0442\u0430\u0432\u043b\u044f\u0442\u044c \u0431\u0430\u043d\u043d\u0435\u0440\u044b \u043a\u043b\u0430\u0441\u0441 - .add_banner\r\n     * @param callback - \u043a\u043e\u0433\u0434\u0430 \u0432\u0441\u0435 \u0431\u0430\u043d\u043d\u0435\u0440\u044b \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u044b \u043e\u0442\u0434\u0430\u0435\u043c \u0432 callback\r\n     */\r\n    function addBanner(callback) {\r\n      var all_height = $('.container-wrap').innerHeight();\r\n      if(!all_height){\r\n        all_height = $('.l-outer').innerHeight();//priceslist - \u0442\u0430\u043c \u043d\u0435\u0442 .container-wrap\r\n      }\r\n      var right_height = $('.col3').innerHeight();\r\n      if(!right_height){\r\n        $('.w-col.m-last > *').each(function () {\r\n          right_height = +right_height + $(this).innerHeight();\r\n        });//priceslist - \u0442\u0430\u043c \u043d\u0435\u0442 .col3\r\n      }\r\n      /*\u0412 \u043e\u0431\u044a\u044f\u0432\u043b\u0435\u043d\u0438\u044f\u0445 \u043d\u0430 \u043c\u043e\u0431\u0438\u043b\u043a\u0435 \u0431\u0430\u043d\u043d\u0435\u0440\u044b \u0443\u0445\u043e\u0434\u044f\u0442 \u0432 \u043b\u0435\u0432\u0443\u044e \u043a\u043e\u043b\u043e\u043d\u043a\u0443*/\r\n      if(isMobile.any() && location.pathname.indexOf('trade')!=-1){\r\n        all_height = all_height - $('.col2').innerHeight();\r\n      }\r\n      if(all_height<0){\r\n        return false;\r\n      }\r\n      var result = Math.floor((all_height - right_height) / 348);\r\n      var list_banner = [];\r\n      $('.rttr_bnr').each(function () {\r\n        if ($(this).data('id')) {\r\n          list_banner.push($(this).data('id'));\r\n        }\r\n      });\r\n\r\n      if (result > 0 && $('.add_banner').length) {\r\n        var alias_str = 'only_rotation';\r\n        var allSeats = +$('.rttr_bnr').length + +result;\r\n        if (allSeats > 10) {\r\n          result = 10 - $('.rttr_bnr').length;\r\n        }\r\n        $.ajax({\r\n          type: \"POST\",\r\n          url: \"http://foodretail.ru/rotatorstat/getBanner\",\r\n          dataType: 'html',\r\n\r\n          data: {\r\n            'alias': alias_str,\r\n            'list_banner': list_banner,\r\n            'limit': result,\r\n            'section' : $('.add_banner').data('section')\r\n          },\r\n          success: function (html) {\r\n            $('.add_banner:first').append($(html));\r\n            var banners = [];\r\n            $(html).each(function () {\r\n              banners.push($(this).find('.rttr_bnr'));\r\n            });\r\n            callback(banners);\r\n          }\r\n        });\r\n      } else {\r\n        callback(true);\r\n      }\r\n    }\r\n  });\r\n  var isMobile = {\r\n    Android: function() {\r\n      return navigator.userAgent.match(/Android/i);\r\n    },\r\n    BlackBerry: function() {\r\n      return navigator.userAgent.match(/BlackBerry/i);\r\n    },\r\n    iOS: function() {\r\n      return navigator.userAgent.match(/iPhone|iPad|iPod/i);\r\n    },\r\n    Opera: function() {\r\n      return navigator.userAgent.match(/Opera Mini/i);\r\n    },\r\n    Windows: function() {\r\n      return navigator.userAgent.match(/IEMobile/i);\r\n    },\r\n    any: function() {\r\n      return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());\r\n    }\r\n  };\r\n</script>\n\n<script type=\"text/javascript\">\n  $(document).ready(function() {\n    $scrollToTop = \"<p id=\\\"scrollToTop\\\"><a href=\\\"#\\\">\u041d\u0430\u0432\u0435\u0440\u0445</a></p>\";\n\n    $(\".l-main\").append($scrollToTop);\n\n    if($(window).scrollTop() > 350) {\n      $('#scrollToTop').fadeIn();\n    }\n\n    $(window).scroll(function () {\n      if ($(this).scrollTop() > 350) {\n        $('#scrollToTop').fadeIn();\n      } else {\n        $('#scrollToTop').fadeOut();\n      }\n    });\n\n    $('#scrollToTop a').click(function () {\n      $('body,html').animate({\n        scrollTop: 0\n      }, 20);\n      return false;\n    });\n\n    // \u0421\u0432\u0435\u0440\u0442\u044b\u0432\u0430\u043d\u0438\u0435 \u043c\u0435\u043d\u044e \u043f\u0440\u043e\u0444\u0438\u043b\u044f\n    var userMenuLi = $(\"#profile_menu .profile_menu li\");\n    userMenuLi.not(\".shortMenu\").hide();\n\n    $(\".profile_menu .link\").click(function() {\n      var that = $(this);\n      if(that.hasClass(\"link-open\")) {\n        userMenuLi.filter(\":hidden\").slideToggle(70);\n        textLink = \"<a href=\\\"#\\\"><strong>\u0421\u0432\u0435\u0440\u043d\u0443\u0442\u044c \u043f\u0443\u043d\u043a\u0442\u044b</strong>&nbsp;<span>&#9650;</span></a>\";\n        that.removeClass(\"link-open\").addClass(\"link-close\").html(textLink);\n      } else {\n        userMenuLi.not(\".shortMenu\").slideToggle(70);\n        textLink = \"<a href=\\\"#\\\"><strong>\u0415\u0449\u0435 \u043f\u0443\u043d\u043a\u0442\u044b</strong>&nbsp;<span>&#9660;</span></a>\";\n        that.removeClass(\"link-close\").addClass(\"link-open\").html(textLink);\n      }\n      return false;\n    });\n\n    if(!Modernizr.input.placeholder){\n\n      $('[placeholder]').focus(function() {\n        var input = $(this);\n        if (input.val() == input.attr('placeholder')) {\n        input.val('');\n        input.removeClass('placeholder');\n        }\n      }).blur(function() {\n        var input = $(this);\n        if (input.val() == '' || input.val() == input.attr('placeholder')) {\n        input.addClass('placeholder');\n        input.val(input.attr('placeholder'));\n        }\n      }).blur();\n      $('[placeholder]').parents('form').submit(function() {\n        $(this).find('[placeholder]').each(function() {\n        var input = $(this);\n        if (input.val() == input.attr('placeholder')) {\n          input.val('');\n        }\n        })\n      });\n\n    }\n\n    $(document.body).addClass(\"m16\");\n\n    var col2Array = [];\n    var col3Array = [];\n    $(\".col2 .rotator\").each(function() { col2Array.push(this) });\n    $(\".col3 .rotator\").each(function() { col3Array.push(this) });\n\n\n    function bannersOnResize(col2Array, col3Array) {\n\n      if(col3Array.length > 0) {\n\n        if (document.documentElement.clientWidth < 1020) {\n\n          var resultArray = [];\n          var col2ArrayCopy = col2Array.slice();\n          var col3ArrayCopy = col3Array.slice();\n          while (col2ArrayCopy.length || col3ArrayCopy.length) {\n            if (col2ArrayCopy.length) {\n              resultArray.push(col2ArrayCopy.shift());\n            }\n            if (col3ArrayCopy.length) {\n              resultArray.push(col3ArrayCopy.shift());\n            }\n          }\n          $(\".col2 .rotator, .col3 .rotator\").remove();\n\n          $(\".col2\").append($(resultArray));\n\n        } else {\n\n          $(\".col2 .rotator\").remove();\n\n          $(\".col2\").append($(col2Array));\n          $(\".col3\").append($(col3Array));\n\n        }\n\n      }\n\n    }\n\n    bannersOnResize(col2Array, col3Array);\n\n    $(window).resize(function() {\n      bannersOnResize(col2Array, col3Array);\n    });\n\n    \n  });\n\n</script>\n\n\n\n  \n\n\n\n  </div><!-- /.l-wrapper-->\n    \n</body>\n</html>"